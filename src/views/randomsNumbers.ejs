<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta
			http-equiv="X-UA-Compatible"
			content="IE=edge"
		/>
		<meta
			name="viewport"
			content="width=device-width, initial-scale=1.0"
		/>
		<title>Randoms Numbers</title>
	</head>
	<body>
		<h1>Randoms numbers</h1>

		<table>
			<thead>
				<tr>
					<th>Propiedad</th>
					<th>Valor</th>
				</tr>
			</thead>
			<tbody>
				<% let objeto = msg; %> <% for (let propiedad in objeto) { %>
				<tr>
					<td><%= propiedad %></td>
					<td><%= objeto[propiedad] %></td>
				</tr>
				<% } %>
			</tbody>
		</table>
	</body>
</html>
